{
    "name": "itexmo/itexmo-sms",
    "description": "Itexmo SMS Gateway Integration for Laravel",
    "type": "library",
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "5.8.*",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "orchestra/testbench": "3.8.*"
    },
    "autoload": {
        "psr-4": {
            "Itexmo\\ItexmoSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itexmo\\ItexmoSms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itexmo\\ItexmoSms\\ItexmoSmsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Agnes Mbiti",
            "email": "agypeter97@gmail.com"
        }
    ]
}
